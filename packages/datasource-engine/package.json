{
  "name": "@easy-editor/datasource-engine",
  "version": "0.0.1",
  "description": "DataSource Engine for EasyEditor",
  "type": "module",
  "main": "src/index.ts",
  "files": [
    "dist",
    "LICENSE",
    "README.md"
  ],
  "publishConfig": {
    "access": "public",
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "typings": "dist/index.d.ts",
    "module": "dist/index.js",
    "exports": {
      ".": {
        "import": "./dist/index.js",
        "require": "./dist/cjs/index.js",
        "default": "./dist/index.js"
      }
    }
  },
  "homepage": "https://github.com/Easy-Editor/EasyEditor",
  "license": "MIT",
  "author": "JinSo <kimjinso@qq.com>",
  "keywords": [
    "@easy-editor",
    "easyeditor",
    "low-code",
    "editor",
    "engine"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/Easy-Editor/EasyEditor"
  },
  "bugs": {
    "url": "https://github.com/Easy-Editor/EasyEditor/issues"
  },
  "scripts": {
    "format": "biome format --write .",
    "lint": "biome check .",
    "build": "npm-run-all -nl build:*",
    "build:clean": "rimraf dist/",
    "build:js": "rollup -c",
    "types": "npm-run-all -nl types:*",
    "types:src": "tsc --project tsconfig.build.json",
    "test-types": "tsc --project tsconfig.test.json"
  },
  "peerDependencies": {
    "@easy-editor/core": "^0.0.15",
    "@easy-editor/react-renderer": "^0.0.17"
  },
  "dependencies": {
    "lodash-es": "^4.17.21"
  },
  "devDependencies": {
    "@types/lodash-es": "^4.17.12"
  }
}
